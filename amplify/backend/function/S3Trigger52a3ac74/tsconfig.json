{
  "include": [
    "lib",
    "lib/qlip-crop-model-out.json"
  ],
  "compilerOptions": {
    "target": "esnext",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "esnext"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "outDir": "./src",
    "baseUrl": "./",
    "rootDir": "./lib",
    "paths": {
      "src": [
        "./lib"
      ]
    }
  }
}